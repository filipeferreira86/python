RpnCalculatorForm:

<RpnCalculatorForm>:
    orientation: 'vertical'

    registrador_x:registrador_x
    registrador_y:registrador_y
    registrador_z:registrador_z
    registrador_t:registrador_t

    BoxLayout:
        orientation: 'vertical'
        cols: 1
        spacing: 5
        padding: [200, 10]

        BoxLayout:
            orientation: 'horizontal'

            Label:
                text:'Reg T'
                size_hint_x: .5
            Label:
                text: '0'
                id: registrador_t
                size_hint_x: 1


        BoxLayout:
            orientation: 'horizontal'

            Label:
                text:'Reg Z'
                size_hint_x: .5
            Label:
                text: '0'
                id: registrador_z
                size_hint_x: 1


        BoxLayout:
            orientation: 'horizontal'

            Label:
                text:'Reg Y'
                size_hint_x: .5
            Label:
                text: '0'
                id: registrador_y
                size_hint_x: 1

        BoxLayout:
            orientation: 'horizontal'

            Label:
                text: 'Reg X'
                size_hint_x: .5
            Label:
                text:'0'
                size_hint_x: 1
                id: registrador_x


    GridLayout:
        cols: 3
        spacing: 10
        padding: [20, 10]

        Button:
            text: '1'
            on_press: root.entradigito(1)
            id: but1
        Button:
            text: '2'
            on_press: root.entradigito(2)
            id: but2
        Button:
            text: '3'
            on_press: root.entradigito(3)
            id: but3
        Button:
            text: '4'
            on_press: root.entradigito(4)
            id: but4
        Button:
            text: '5'
            on_press: root.entradigito(5)
            id: but5
        Button:
            text: '6'
            on_press: root.entradigito(6)
            id: but6
        Button:
            text: '7'
            on_press: root.entradigito(7)
            id: but7
        Button:
            text: '8'
            on_press: root.entradigito(8)
            id: but18
        Button:
            text: '9'
            on_press: root.entradigito(9)
            id: but9
        Button:
            text: '+'
            on_press: root.processa_mais()
            id: butmais
        Button:
            text: '0'
            on_press: root.entradigito(0)
            id: but0
        Button:
            text: '-'
            on_press: root.processa_Menos()
            id: butmenos
        Button:
            text: 'x'
            on_press: root.processaVezes()
            id: butvezes
        Button:
            text: 'ENTER'
            on_press: root.processa_enter()
            id: butenter
        Button:
            text: '/'
            on_press: root.processaDivide()
            id: butdiv