RelativeLayout:
    canvas:
        Point:
            points: 50, 50, 60, 50, 70, 50, 80, 50
            pointsize: 4

        Triangle:
            points: 100, 100, 150, 150, 200, 100

        Quad:
            points: 300, 100, 300, 200, 400, 200, 400, 100

        Line:
            points: 500, 100, 550, 150, 600, 100, 650, 150

        Bezier:
            points: 500,300, 550,350, 600,300, 650,350
            segments: 360
            dash_length: 2
            dash_offset: 4